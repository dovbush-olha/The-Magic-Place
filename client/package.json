{
  "name": "@magic-place/client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "prettify": "prettier --log-level warn --cache --write .",
    "types": "tsc --noEmit --project ./tsconfig.node.json && tsc --noEmit --project ./tsconfig.app.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "generate": "plop",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache"
  },
  "dependencies": {
    "@fontsource-variable/geologica": "^5.0.6",
    "@magic-place/server": "workspace:*",
    "@tanstack/react-query": "^5.51.15",
    "@trpc/client": "^11.0.0-rc.467",
    "@trpc/react-query": "^11.0.0-rc.467",
    "@tsparticles/preset-stars": "^3.1.0",
    "@tsparticles/react": "^3.0.0",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.26.0",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@tsparticles/engine": "^3.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "happy-dom": "^15.7.4",
    "identity-obj-proxy": "^3.0.0",
    "plop": "^4.0.1",
    "storybook": "^8.2.9",
    "storybook-addon-remix-react-router": "^3.0.1",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "vite": "^5.3.4",
    "vite-plugin-svg-spriter": "^1.0.0",
    "vitest": "^2.1.1"
  }

{
  "name": "@magic-place/server",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --rs src/index.ts",
    "build": "rimraf ./dist && tsc --build ./tsconfig.json",
    "start": "node ./dist/index.js",
    "prettify": "prettier --loglevel warn --cache --write . -ext .json,.ts,.tsx,.js,.jsx,.css,.scss,.md,.mdx,.html --ignore-unknown",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts ."
  },
  "dependencies": {
    "@trpc/server": "^11.0.0-rc.467",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0"
  }
}
